a = [j for j in range(1, 1000)]
b = [j for j in range(1, 1000)]
c = [j for j in range(1, 1000)]

print a

for x in a:
    for y in b:
        for z in c:
            if x**2 + y**2 == z**2:
                if x + y + z == 1000:
                    print x * y * z
                else:
                    continue
            else:
                continue
